{
  "name": "textlint-plugin-review-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/srz-zumix/textlint-plugin-review-starter"
  },
  "author": "srz_zumix",
  "email": "zumix.cpp@gmail.com",
  "homepage": "https://github.com/srz-zumix/textlint-plugin-review-starter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/srz-zumix/textlint-plugin-review-starter/issues"
  },
  "version": "0.1.4",
  "description": "textlint Re:VIEW Starter processsor plugin.",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir lib --source-maps",
    "watch": "NODE_ENV=production babel src --out-dir lib --watch --source-maps",
    "prepublish": "npm run --if-present build",
    "test": "mocha --require @babel/register"
  },
  "keywords": [
    "textlint",
    "plugin",
    "review",
    "re:view",
    "review-starter",
    "re:view-starter",
    "lint"
  ],
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/register": "^7.15.3",
    "@textlint/kernel": "^12.0.2",
    "babel-plugin-espower": "^3.0.1",
    "babel-preset-power-assert": "^3.0.0",
    "eslint": "^8.1.0",
    "mocha": "^9.1.1",
    "power-assert": "^1.6.1",
    "textlint": "^12.0.2",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-rule-no-todo": "^2.0.1"
  },
  "dependencies": {
    "@textlint/ast-tester": "^13.1.0",
    "@textlint/ast-traverse": "^12.1.0"
  }
}
